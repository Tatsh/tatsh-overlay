{
	"author": "Andrew Udvare <audvare@gmail.com>",
	"devDependencies": {
		"cspell": "^6.31.1",
		"prettier": "^2.8.7",
		"prettier-plugin-ini": "^1.1.0",
		"prettier-plugin-toml": "^0.3.1"
	},
	"license": "MIT",
	"name": "instagram-archiver",
	"repository": "git@github.com:Tatsh/instagram-archiver.git",
	"scripts": {
		"check-formatting": "prettier -c './**/*.cfg' './**/*.ini' './**/*.md' './**/*.toml' './**/*.yml' ./.prettierrc ./.style.yapf && isort . --check && yapf -rd scripts",
		"check-spelling": "cspell --no-progress './**/*'  './**/.*'",
		"format": "prettier -w './**/*.cfg' './**/*.ini' './**/*.md' './**/*.toml' './**/*.yml' ./.prettierrc ./.style.yapf && isort . && yapf -ri scripts",
		"install-python-deps": "python -m pip install --upgrade pip && pip install poetry && poetry install && pip install -e . && pip install mypy pylint pylint-quotes pytest pytest-cov coveralls pytest-mock types-requests toml yapf",
		"mypy": "mypy .",
		"pylint": "pylint scripts",
		"qa": "yarn mypy && yarn pylint && yarn check-spelling && yarn check-formatting"
	},
	"version": "0.2.0"
}
